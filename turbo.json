{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["analyze", "build"],
      "outputs": ["dist/**", "elements/**", "elements.json", "vscode.html-data.json"]
    },
    "format": {
      "dependsOn": ["format"],
      "inputs": ["src/**"]
    },
    "test": {
      "dependsOn": ["test", "test:server"],
      "inputs": ["src/**"]
    }
  }
}
